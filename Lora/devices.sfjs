let

    DEVICE: require('./Templates/device.sfjt'),
    
    //
    // Common aspects of an Elsys ERS device in Application App1
    //
    ERS_DEVICE: $DEVICE << {
        "application":   "App1",
        "deviceProfile": "Elsys",    
        "skipFCntCheck": true,
        // Settings to send to the Device
        settings: {
            fPort: 6,
            values: {
                SplPer: 100, 
                TempPer: 3,
                External: 23
            }
        }

    },

    
