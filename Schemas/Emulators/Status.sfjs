    //
    // Status for a low side Emulator
    //
    LOW_EMULATOR: {
        msg:    {type string}
        status: {type: string}
    },

    //
    // Status for the set of Low side Emulators
    //
    LOW_EMULATORS: {
        type: 'object',
        properties: {
