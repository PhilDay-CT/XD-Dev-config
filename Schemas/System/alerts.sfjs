//
// Schema for Alert messages
//

let

    ALERT: {
        type: object,
        required: ['kind', 'alert'],
        additionalProperties: false,
        properties: { 
            kind: {type: string, const: 'alert'},
            alert: {
                type: object,
                additionalProperties: false,
                properties: {
                    type:    {type: string, maxLength:30},
                    subject: {type: string, maxLength:30},
                    origin:  {type: string, maxLength:1000},
                    msg:     {type: string, maxLength:1000},
                    zone:    {type: string, maxLength:30},
                    details: {
                        type: [string,array,object],
                        maxLength: 5000,
                        maxItems: 50,
                        maxProperties: 50 
                    },
                },
            },
            data: {
                type: [object],
                maxItems: 50,
            },
            trace: require('./Schemas/System/Templates/Trace.sfjt')
        },
    },

    ALERT_WITH_STRING_DATA: $ALERT << {
        properties: {
            data: {type: [string]},
        },   
    },

in
{
    alert: {
        'ParseError' : $ALERT_WITH_STRING_DATA,
        '**'         : $ALERT,
    }

}