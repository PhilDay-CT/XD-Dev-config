//sfTimestamp:1769527292390
//sfLabel:Schemas/System/PipelineStatus.sfjs
let
    STATUS: require('./Schemas/System/Templates/Status.sfjt'),

    //
    // Status reported by an element in a Low Side
    // pipline such as a Sender or Receiver
    //
    LOW_PIPELINE_ELEMENT_STATUS: {
        type: 'object',
        properties: {
            details: {
                type: ['object','string'],
                properties: {
                    pipeline: {type: 'string'},
                    group: {type: 'string'},
                    domain: {type: 'string'},
                    node: {type: 'string'},
                    timeout: {type: 'integer'},
                    url: {type: 'string'},
                    error: {type: 'string'},
                    maxLength: 5000,
                },
                additionalProperties: false,
            },
            status: {type: 'string'},
            msg: {type: 'string'},
        },
        additionalProperties: false,    
        required: ['status']
    },

    // Collection of Status elements for a low domain section
    // of a pipeline
    LOW_PIPELINE_STATUS: {
        
                type: 'object',
                patternProperties: {
                    "^[a-zA-Z0-9\\-\\_\\ \\(\\)]*$": {
                        type: 'object',
                        patternProperties: {
                            "^Receiver:.*|^Sender:.*|^Low.*": $LOW_PIPELINE_ELEMENT_STATUS
                        },
                        additionalProperties: false,
                    },
                },
                additionalProperties: false,
       
    },  

    // Pipeline Status Schema
    LOW_PIPELINES: {
        type: 'object',
        properties: {
            _legend: {type: 'object'},
            _kind:   {type: 'string', const: 'Pipeline'},
        },
        patternProperties: {
           "^Import$|^Export$|^System$": $LOW_PIPELINE_STATUS,
        },
        additionalProperties: false,
    },

    //  Pipeline Status schame when running with zones
    LOW_ZONE_PIPELINES: {
        type: 'object',
        patternProperties: {
           "^Zone.*$": $LOW_PIPELINES
        },
        additionalProperties: false,
    }  
  
in
{
    sfjs: {
        status: ($STATUS << {
            status_schema: {
                Pipelines:  $LOW_ZONE_PIPELINES,
            }
        }).schema
    }
}   
