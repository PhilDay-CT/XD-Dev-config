let
    STATUS: require('./Schemas/System/Templates/Status.sfjt'),

    //
    // Low side Status for a single Pipeline
    //
    LOW_PIPELINE_STATUS: {
        type: 'object',
        properties: {
            details: {
                type: ['object','string'],
                properties: {
                    pipeline: {type: 'string'},
                    group: {type: 'string'},
                    domain: {type: 'string'},
                    node: {type: 'string'},
                    timeout: {type: 'integer'},
                    maxLength: 5000,
                },
                additionalProperties: false,
            },
            status: {type: 'string'},
            msg: {type: 'string'},
        },
        additionalProperties: false,    
        required: ['status']
    },


    // Status for the set of Pipelines deployed by the system
    LOW_PIPELINES: {
        type: 'object',
        properties: {
            _legend: {type: 'object'},
            _kind:   {type: 'string', const: 'Pipeline'},
        },
        patternProperties: {
           "^Import$|^Export$|^System$": {
                type: 'object',
                patternProperties: {
                    "^[a-zA-Z0-9\\-\\_\\ \\(\\)]*$": {
                        type: 'object',
                        patternProperties: {
                            "^Receiver:.*|^Sender:.*|^Low.*": $LOW_PIPELINE_STATUS
                        },
                        additionalProperties: false,
                    },
                },
                additionalProperties: false,
            }
        },
        additionalProperties: false,
    }  
in
{
    sfjs: {
        status: ($STATUS << {
            model: {
                properties: {
                    system : {
                        properties: {
                            status: {
                                properties : {
                                    Pipelines:  $LOW_PIPELINES,
                                }
                            }
                        }
                    }
                }
            }
        }).schema
    }
}   
