//sfTimestamp:1771410241967
//sfLabel:Schemas/System/Templates/Domain.sfjt
//
// System status form Processs and comonents is reported in a structure
// based on the FQDN, so we capture that in this template.
//   
//   ctxd.low.<hostname>
//
let
    FIXED_OBJECT: {
        type: object,
        additionalProperties: false,
    }
in
{
    model: {},
    domain: $FIXED_OBJECT << {
        required: [ctxd],
        properties: {
            ctxd: $FIXED_OBJECT << {
                properties: {
                    low: $FIXED_OBJECT << {
                        patternproperties: {
                            "^[A-Za-z0-9][A-Za-z0-9-_]*$": @model
                        } 
                    }
                }
            }
        }
    }
}