let
    SFJS_MODEL: require('./Schemas/System/Templates/sfjsModel.sfjt'),
in

// Template for Low Side status model
//
// Extend by defining the model element and then using .model
//
// For example:
//
// let
//     STATUS: require('./Schemas/System/Templates/Status.sfjt')
// in
// {   
//    myStatus: ($STATUS << {
//        status_model:  <schema for the status>
//    }).schema 
//

$SFJS_MODEL << {
    status_schema: {},
    model: {
        type: object,
        required: [system],
        additionalProperties: false,
        properties: {
            system: {
                type: object,
                required: [status],
                additionalProperties: false,
                properties: {
                    status: {
                        type: object,
                        additionalProperties: false,
                        properties: @status_schema
                    },
                },
            },
        },
    },
}
