//sfTimestamp:1769693181547
//sfLabel:Schemas/System/ComponentStatus.sfjs
let
    STATUS: require('./Schemas/System/Templates/Status.sfjt'),
    LEGEND: require('./Schemas/System/Templates/Status_Legend.sfjt'),

    //
    // Comonent Legend
    //
    COMPONENT_LEGEND: {
        type: 'object',
        additionalProperties: false, 
        properties: {
            Component: $LEGEND,
        }
    },

    //
    // Low side Component Status
    //
    LOW_COMPONENTS: {
        type: 'object',
        additionalProperties: false,
        properties: {
            _legend: $COMPONENT_LEGEND,
        },
        patternProperties: {
            "^low_": {
                type: 'object',
                additionalProperties: false, 
                properties: {
                    Components: {
                        type: object,
                        additionalProperties: false,
                        properties: {
                            _kind:   {type: 'string', const: 'Component'},
                        },
                        // Status entry for components - these are nested
                        // by the deployment model so we can't define
                        // any more structure here   
                        patternProperties: {
                            "^[a-zA-Z0-9][a-zA-Z0-9_-]+$": {type: object}
                        },
                    }
                },
            }
        },
    },

    LOW_ZONE_COMPONENTS: {
        type: 'object',
        additionalProperties: false,
        patternProperties: {
           "^Zone.*$": $LOW_COMPONENTS
        },
    }  

in
{
    sfjs: {
        status: ($STATUS << {
            status_schema: {
                System: $LOW_ZONE_COMPONENTS
            }
        }).schema
    }
}   
