    MQTT: $MQTT_RECEIVER << {
        topics: ['lora/em1/EmType1/#'],
        format: 'json',
        targets: {
            default: Sender,
        },
    },
 
    Sender: $XD_SENDER << {
        id: 'em-type1',
        messageKinds: ['mqtt'],
        pingInterval: 10*1000
    },
}
